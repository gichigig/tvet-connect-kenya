# BigBlueButton Configuration
# Add these to your .env file

# BigBlueButton Server URL (without trailing slash)
VITE_BBB_URL=https://your-bbb-server.com/bigbluebutton

# BigBlueButton Server Secret Key
VITE_BBB_SECRET=your-bbb-secret-key

# Optional: Default meeting settings
VITE_BBB_DEFAULT_WELCOME_MESSAGE=Welcome to TVET Connect Kenya Virtual Classroom!
VITE_BBB_DEFAULT_MAX_PARTICIPANTS=50
VITE_BBB_DEFAULT_DURATION=120

# Optional: Branding
VITE_BBB_LOGO_URL=https://your-domain.com/logo.png
VITE_BBB_COPYRIGHT_TEXT=Â© 2024 TVET Connect Kenya
