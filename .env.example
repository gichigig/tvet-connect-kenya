# Storage Configuration
# Choose storage provider: 'b2' for Backblaze B2 or 'r2' for Cloudflare R2
STORAGE_PROVIDER=b2

# Backblaze B2 Configuration
B2_ACCOUNT_ID=your_account_id
B2_APPLICATION_KEY=your_application_key
B2_BUCKET_ID=your_bucket_id
B2_BUCKET_NAME=your_bucket_name
B2_ENDPOINT=https://f002.backblazeb2.com/file/your-bucket-name

# Cloudflare R2 Configuration
R2_ACCOUNT_ID=your_account_id
R2_ACCESS_KEY_ID=your_access_key_id
R2_SECRET_ACCESS_KEY=your_secret_access_key
R2_BUCKET_NAME=your_bucket_name
R2_CUSTOM_DOMAIN=https://your-custom-domain.com # Optional

# Database Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key # For admin operations

# AWS Configuration (legacy - to be migrated)
VITE_AWS_REGION=us-east-1
VITE_***REMOVED***=your_access_key_id_here
VITE_***REMOVED***=your_secret_access_key_here
VITE_S3_BUCKET_NAME=tvet-connect-kenya

# AWS Lambda API Gateway URL (legacy - to be migrated)
VITE_AWS_LAMBDA_API_URL=https://your-api-gateway-id.execute-api.us-east-1.amazonaws.com/prod

# Gmail Integration Configuration
# To enable Gmail integration, you need to:
# 1. Create a Google Cloud Console project
# 2. Enable the Gmail API
# 3. Create OAuth 2.0 credentials
# 4. Add these values to your .env file
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_API_KEY=your_google_api_key_here

